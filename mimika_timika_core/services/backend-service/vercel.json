{
  "version": 2,
  "description": "Papua News Backend",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  },
  "crons": [
    {
      "path": "/api/cron/scrape",
      "schedule": "0 * * * *"
    }
  ],
  "functions": {
    "api/index.py": {
      "maxDuration": 60
    }
  }
}