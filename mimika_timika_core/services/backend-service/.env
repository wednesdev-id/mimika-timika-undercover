# News Scraper Configuration

# Output format: csv or excel
OUTPUT_FORMAT=json

# Scheduler settings
SCHEDULER_MODE=daily
SCHEDULER_TIME=08:00
SCHEDULER_INTERVAL=60

# Data and log paths
DATA_PATH=./data
LOG_PATH=./logs

# Scraping settings
REQUEST_TIMEOUT=10
MIN_DELAY=1
MAX_DELAY=3
MAX_ARTICLES_PER_SECTION=10

# Logging level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO